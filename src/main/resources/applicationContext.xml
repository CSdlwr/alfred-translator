<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beanFactoryHolder" class="lvluming.util.BeanFactoryHolder"/>

    <bean id="translator" class="lvluming.processor.YoudaoTranslator"/>

    <bean id="parser" class="lvluming.processor.YoudaoTranslationParser"/>
    <bean id="viewer" class="lvluming.processor.SimpleXmlViewer"/>

    <bean id="viewResolver" class="lvluming.processor.AlfredViewResolver">
        <constructor-arg index="0" ref="parser"/>
        <constructor-arg index="1" ref="viewer"/>
    </bean>

</beans>